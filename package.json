{
  "name": "create-tin",
  "version": "2.5.0",
  "description": "Modern Express.js project generator with TypeScript/JavaScript templates, MongoDB integration, flexible authentication options, and Docker support",
  "main": "dist/bin/index.js",
  "type": "module",
  "bin": {
    "create-tin": "dist/bin/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && tsc && npm run copy-templates && npm run clean-templates",
    "copy-templates": "node scripts/copy-templates.mjs",
    "clean-templates": "node scripts/clean-templates.mjs",
    "start": "node dist/bin/index.js",
    "dev": "tsc && node dist/bin/index.js",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "files": [
    "dist/",
    "dist/bin/",
    "dist/templates/",
    "templates/js/cookies/**/!(node_modules)",
    "templates/js/jwt/**/!(node_modules)",
    "templates/ts/cookies/**/!(node_modules)",
    "templates/ts/jwt/**/!(node_modules)",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "express",
    "cli",
    "scaffold",
    "typescript",
    "javascript",
    "boilerplate",
    "jwt",
    "authentication",
    "cookies",
    "mongodb",
    "create",
    "generator",
    "node",
    "nodejs",
    "express-generator",
    "api",
    "rest-api",
    "starter-kit",
    "express-boilerplate",
    "nodejs",
    "web-development",
    "backend",
    "server",
    "docker",
    "mongoose",
    "express-typescript",
    "express-javascript",
    "backend-generator",
    "api-generator",
    "tin",
    "create-tin",
    "swagger-ui-express"
  ],
  "author": {
    "name": "Aneesh Sunganahalli",
    "url": "https://github.com/aneeshsunganahalli"
  },
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aneeshsunganahalli/Tin"
  },
  "bugs": {
    "url": "https://github.com/aneeshsunganahalli/Tin/issues"
  },
  "homepage": "https://github.com/aneeshsunganahalli/Tin#readme",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.6.3",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.0.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
